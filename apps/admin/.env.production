# backend service base url, production environment
# Configure this to your production backend URL
VITE_SERVICE_BASE_URL=/api

# other backend service base url, production environment
VITE_OTHER_SERVICE_BASE_URL= `{
  "demo": "http://localhost:9529"
}`

# disable http proxy in production mode (use nginx reverse proxy)
VITE_HTTP_PROXY=N

# auth route mode: static ï½œ dynamic
# use dynamic mode to load routes from backend
VITE_AUTH_ROUTE_MODE=dynamic

# success code of backend service (RuoYi-Vue-Plus uses 200 for success)
VITE_SERVICE_SUCCESS_CODE=200

# logout codes of backend service
VITE_SERVICE_LOGOUT_CODES=401

# modal logout codes of backend service
VITE_SERVICE_MODAL_LOGOUT_CODES=

# token expired codes of backend service
VITE_SERVICE_EXPIRED_TOKEN_CODES=401

# sourcemap disabled in production
VITE_SOURCE_MAP=N
